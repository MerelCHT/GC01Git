package pieces;

public class Rook extends AbstractPiece {
	
	public Rook(boolean isWhite){
		super(isWhite);
	}
		
	public int relativeValue(){
		return 5;
	}
	
	public void draw(){
		
	}
	
	public boolean isMoveValid(int srcRow, int srcCol, int destRow, int destCol){
		if (destRow != srcRow || destCol != srcCol)
		{
			return true;
		}
	}

}
